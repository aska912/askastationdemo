            <type>scancode_waitmsg</type>
            <key>scan_sub_menu</key>
        </subbutton>
    </button>
    <button name="发送">
        <subbutton name="扫一扫">
            <type>scancode_push</type>
            <key>send_menu_1_1</key>
        </subbutton>
        <subbutton name="位置">
            <type>location_select</type>
            <key>send_menu_1_2</key>
        </subbutton>
        <subbutton name="照片">
            <type>pic_weixin</type>
            <key>send_menu_1_3</key>
        </subbutton>
        <subbutton name="拍摄">
            <type>pic_sysphoto</type>
            <key>send_menu_1_4</key>
        </subbutton>
        <subbutton name="照片or拍摄">
            <type>pic_photo_or_album</type>
            <key>send_menu_1_5</key>
        </subbutton>
    </button>
	  <matchrule>
	      <!--用户标签的id，可通过用户标签管理接口获取-->
	      <tag_id></tag_id>
		    <!-- 男（1）女（2）-->
		    <sex>1</sex>
		    <!--国家信息，是用户在微信中设置的地区，具体请参考地区信息表-->
		    <country></country>
		    <!--省份信息-->
		    <province></province>
		    <!--城市信息-->
		    <city></city>
		    <!--client_platform_type: ios(1), android(2), other(3)-->
		    <client_platform_type>1</client_platform_type>
		    <language>zh_CN</language>
	  </matchrule>
</menu>

